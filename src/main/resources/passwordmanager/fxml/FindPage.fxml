<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox minHeight="600.0" minWidth="800.0" styleClass="background" stylesheets="@../css/DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="passwordmanager.ui.controllers.FindPageController">
   <children>
      <GridPane fx:id="searchFunctionsGridPane" minHeight="125.0" styleClass="search-functions-bar">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
          <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="searchEntryBox" alignment="CENTER" minWidth="200.0" promptText="example@gmail.com" styleClass="text-entry" GridPane.rowIndex="1">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <GridPane.margin>
                  <Insets bottom="10.0" left="40.0" right="10.0" top="10.0" />
               </GridPane.margin>
            </TextField>
            <Label fx:id="searchLabel" alignment="BASELINE_LEFT" styleClass="header-label" text="Search">
               <GridPane.margin>
                  <Insets bottom="10.0" left="40.0" right="10.0" top="10.0" />
               </GridPane.margin>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </GridPane>
      <ScrollPane fx:id="searchResultsScrollPane" fitToWidth="true" hbarPolicy="NEVER" minHeight="600.0" minWidth="800.0" styleClass="background" vbarPolicy="ALWAYS" VBox.vgrow="ALWAYS">
         <content>
            <GridPane fx:id="searchResultDisplay" hgap="10.0" minHeight="800.0" minWidth="600.0" styleClass="background" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="50.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="50.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints vgrow="SOMETIMES" />
              </rowConstraints>
               <padding>
                  <Insets bottom="200.0" left="5.0" right="5.0" top="15.0" />
               </padding>
            </GridPane>
         </content>
      </ScrollPane>
   </children>
</VBox>
