            <TextField fx:id="searchEntryBox" alignment="CENTER" minWidth="200.0" promptText="example@gmail.com" styleClass="text-entry" GridPane.rowIndex="1">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
